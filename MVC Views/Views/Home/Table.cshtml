@model CsvRecord

<div class="panel panel-success">
    <div class="panel-heading"><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span></div>
    <div class="panel-body">
        <table class="table">
            @foreach (var rec in Model.Records)
            {
                <tr>
                    @foreach (var item in rec)
                    {
                        <td>@item</td>
                    }
                </tr>
            }
        </table> 
    </div>

</div>

 